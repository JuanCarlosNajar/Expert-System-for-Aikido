<owl:NamedIndividual rdf:about="#transforma_el_pensamiento">
  <rdf:type rdf:resource="#Actividades_sociales_emocionales"/>
  <act_tiene_contraindicación rdf:resource="#e9a10"/>
  <act_tiene_dificultad rdf:resource="#Medio"/>
  <act_tiene_fundamento rdf:resource="#fundamentos_sociales_y_emocionales"/>
  <act_tiene_modalidad rdf:resource="#actividad_individual"/>
  <act_tiene_modalidad rdf:resource="#actividad_juego"/>
  <act_tiene_objetivo rdf:resource="#obj_transformar_pensamiento_negativo"/>
  <usa_capacidad rdf:resource="#regulación_emocional"/>
  <tiene_beneficio rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ayuda a los niños a identificar pensamientos distorsionados y generar una actitud más resiliente y autónoma.</tiene_beneficio>
  <tiene_desarrollo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Después de una actividad frustrante o una caída, el instructor propone un juego:
- Presenta un pensamiento negativo común (“No me sale, soy malo”).
- Los niños deben convertirlo en una versión más útil y realista:
   “No me ha salido hoy, pero puedo mejorar.”
    “Es difícil, pero ya he aprendido otras cosas.”
También se habla de causas realistas: “¿Qué ha pasado? ¿Qué depende de ti?”</tiene_desarrollo>
  <tiene_nombre rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Transforma el pensamiento</tiene_nombre>
</owl:NamedIndividual>

