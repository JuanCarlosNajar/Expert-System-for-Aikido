#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#desarrolla_habilidad_social_emocional
:desarrolla_habilidad_social_emocional rdf:type owl:ObjectProperty ;
                                       rdfs:domain :Valores ;
                                       rdfs:range :Habilidades_sociales_y_emocionales ;
                                       rdfs:comment "Indica que habilidades sociales y emocionales se desarrollan poniendo en práctica estos valores éticos y morales." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#edad
:edad rdf:type owl:ObjectProperty ;
      rdfs:domain :Impacto_por_edad ;
      rdfs:range :Etapas_edad .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#es_alumno_de
:es_alumno_de rdf:type owl:ObjectProperty ;
              owl:inverseOf :tiene_alumno ;
              rdfs:domain :Alumnos ;
              rdfs:range :Grupos ;
              rdfs:comment "Indica a qué grupo pertenece un alumno." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#es_profesor_de
:es_profesor_de rdf:type owl:ObjectProperty ;
                owl:inverseOf :tiene_profesor ;
                rdfs:domain :Profesores ;
                rdfs:range :Grupos ;
                rdfs:comment "indica a qué grupo enseña el profesor." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#forma_parte_de
:forma_parte_de rdf:type owl:ObjectProperty ;
                rdfs:domain :Etapas_clase ;
                rdfs:range :Estructuras_clase ;
                rdfs:comment "Indica a cual \"estructura de clase\" pertenece la etapa de clase especificada." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#funcion
:funcion rdf:type owl:ObjectProperty ;
         rdfs:domain :Impacto_por_edad ;
         rdfs:range :Funciones_corporales ;
         rdfs:comment "Funciones corporales, según definición del CIF de la OMS, sobre las que tiene algún tipo de impacto, positivo o negativo." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#grupo_edad
:grupo_edad rdf:type owl:ObjectProperty ;
            rdfs:domain :Alumnos ;
            rdfs:range :Etapas_edad ;
            rdfs:comment "grupo de edad al que pertenece una persona" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#impacto
:impacto rdf:type owl:ObjectProperty ;
         rdfs:domain :Impacto_por_edad ;
         rdfs:range :Impacto_funcional ;
         rdfs:comment "Tipo de impacto (favorable o deficiencia) y el grado del impacto." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#impacto_sobre
:impacto_sobre rdf:type owl:ObjectProperty ;
               rdfs:domain :Impacto_funcional ;
               rdfs:range :Funciones_corporales ;
               rdfs:comment "Indica el tipo de impacto (facilitador o deficiencia) tiene sobre las funciones corporales." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#produce_impacto
:produce_impacto rdf:type owl:ObjectProperty ;
                 rdfs:domain :Etapas_edad ;
                 rdfs:range :Impacto_funcional ;
                 rdfs:comment "Indica el tipo de impacto que produce una etapa de edad" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_alumno
:tiene_alumno rdf:type owl:ObjectProperty ;
              rdfs:domain :Grupos ;
              rdfs:range :Alumnos ;
              rdfs:comment "Indica alumno que tiene un grupo." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_apartado
:tiene_apartado rdf:type owl:ObjectProperty ;
                rdfs:domain :Etapas_clase ;
                rdfs:range :Apartados_clase ;
                rdfs:comment "Indica el apartado que tiene una etapa de la clase." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_ataque
:tiene_ataque rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Tecnicas ;
              rdfs:range :Ataques ;
              rdfs:comment "Para indicar el tipo de ataque de uke (katate dori, kata dori, jodan tsuki, etc.)." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_defensa
:tiene_defensa rdf:type owl:ObjectProperty ;
               rdfs:domain :Tecnicas ;
               rdfs:range :Defensas ;
               rdfs:comment "Para indicar la defensa que se va a utilizar contra el ataque." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_forma
:tiene_forma rdf:type owl:ObjectProperty ;
             rdfs:domain :Tecnicas ;
             rdfs:range :Forma_tecnica ;
             rdfs:comment "Para indicar si una técnica se puede realizar en omote y ura." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_habilidad_principal
:tiene_habilidad_principal rdf:type owl:ObjectProperty ;
                           rdfs:domain :Actividades_sociales_y_emocionales ;
                           rdfs:range :Habilidades_sociales_y_emocionales ;
                           rdfs:comment "La habilidad principal que fomenta un ejercicio del grupo de actividades sociales y emocionales." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_habilidad_secundaria
:tiene_habilidad_secundaria rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:domain :Actividades_sociales_y_emocionales ;
                            rdfs:range :Habilidades_sociales_y_emocionales ;
                            rdfs:comment "Indica qué otras habilidades sociales y emocionales se desarrollan con esta actividad." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_impacto_en
:tiene_impacto_en rdf:type owl:ObjectProperty ;
                  rdfs:domain :Etapas_edad ;
                  rdfs:range :Funciones_corporales ;
                  rdfs:comment "Indica las funciones corporales que se ven afectadas por la etapa de la edad" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_posicion
:tiene_posicion rdf:type owl:ObjectProperty ;
                rdfs:domain :Tecnicas ;
                rdfs:range :Posicion ;
                rdfs:comment "Indica las posiciones de uke y tori (ai hanmi y giaku hanmi) uno respecto al otro." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_profesor
:tiene_profesor rdf:type owl:ObjectProperty ;
                rdfs:domain :Grupos ;
                rdfs:range :Profesores ;
                rdfs:comment "Indica el profesor que tiene un grupo" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_valor
:tiene_valor rdf:type owl:ObjectProperty ;
             rdfs:domain :Actividades_valores ;
             rdfs:range :Valores ;
             rdfs:comment "Indica el valor ético y moral que se desarrolla con la actividad." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_waza
:tiene_waza rdf:type owl:ObjectProperty ;
            rdfs:domain :Tecnicas ;
            rdfs:range :Waza ;
            rdfs:comment "Indica el waza al que pertenece la técnica (suwari, hanmi handachi, tachi, ushiro)-" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tipo_defensa
:tipo_defensa rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#usa_capacidad_fisica
:usa_capacidad_fisica rdf:type owl:ObjectProperty ;
                      rdfs:domain :Actividades_fisicas ;
                      rdfs:range :Capacidades_físicas .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#usa_funcion
:usa_funcion rdf:type owl:ObjectProperty ;
             rdfs:domain :Capacidades_físicas ;
             rdfs:range :Funciones_corporales .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#usa_funcion_corporal
:usa_funcion_corporal rdf:type owl:ObjectProperty ;
                      rdfs:domain :Fundamentos_tecnicos ;
                      rdfs:range :Funciones_corporales .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#edad_final
:edad_final rdf:type owl:DatatypeProperty ;
            rdfs:domain :Etapas_edad ;
            rdfs:range xsd:integer ;
            rdfs:comment "Edad final de la etapa de la edad" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#edad_inicial
:edad_inicial rdf:type owl:DatatypeProperty ;
              rdfs:domain :Etapas_edad ;
              rdfs:range xsd:integer ;
              rdfs:comment "Edad inicial de la etapa de la edad" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_beneficio
:tiene_beneficio rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_desarrollo
:tiene_desarrollo rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:string .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_edad
:tiene_edad rdf:type owl:DatatypeProperty ;
            rdfs:domain :Alumnos ,
                        :Profesores ;
            rdfs:range xsd:integer ;
            rdfs:comment "Edad de las personas (alumnos y profesores)" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_enseñanza
:tiene_enseñanza rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :Actividades_valores ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_nombre
:tiene_nombre rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_objetivo
:tiene_objetivo rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#tiene_orden
:tiene_orden rdf:type owl:DatatypeProperty ;
             rdfs:domain :Etapas_clase ;
             rdfs:range xsd:integer ;
             rdfs:comment "El número de orden de la etapa dentro de la estructura de la clase." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#usa_materiales
:usa_materiales rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Actividades_fisicas
:Actividades_fisicas rdf:type owl:Class ;
                     rdfs:subClassOf :Fundamentos_fisicos ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :usa_capacidad_fisica ;
                                       owl:someValuesFrom :Capacidades_físicas
                                     ] ;
                     rdfs:comment "Conjunto de actividades y ejercicios para potenciar las capacidades físicas de los alumnos." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Actividades_sociales_y_emocionales
:Actividades_sociales_y_emocionales rdf:type owl:Class ;
                                    rdfs:subClassOf :Fundamentos_sociales_y_emocionales ;
                                    rdfs:comment "Conjunto de actividades y ejercicios para potenciar las habilidades emocionales y sociales de los alumnos." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Actividades_valores
:Actividades_valores rdf:type owl:Class ;
                     rdfs:subClassOf :Fundamentos_valores ;
                     rdfs:comment "Conjunto de actividades y ejercicios para fomentar los valores éticos y morales en los alumnos." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Agarres
:Agarres rdf:type owl:Class ;
         rdfs:subClassOf :Tipos_ataque ;
         rdfs:comment "Tipos de agarre." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Aikido
:Aikido rdf:type owl:Class .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Alumnos
:Alumnos rdf:type owl:Class ;
         rdfs:subClassOf :Enseñanza ;
         rdfs:comment "Alumnos que participan en las clases." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Apartados_clase
:Apartados_clase rdf:type owl:Class ;
                 rdfs:subClassOf :Enseñanza ;
                 rdfs:comment "Distintos apartados o grupo de actividades que componen una clase" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Ataques
:Ataques rdf:type owl:Class ;
         rdfs:subClassOf :Fundamentos_tecnicos ;
         rdfs:comment "Tipos de ataques de uke." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Capacidades_físicas
:Capacidades_físicas rdf:type owl:Class ;
                     rdfs:subClassOf :Fundamentos_fisicos ;
                     rdfs:comment "Concepto general que engloba todas las capacidades que determinan el rendimiento físico del individuo." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Capacidades_físicas_básicas
:Capacidades_físicas_básicas rdf:type owl:Class ;
                             rdfs:subClassOf :Capacidades_físicas ;
                             rdfs:comment "Son aquellas que determinan directamente el nivel de rendimiento físico del individuo y dependen fundamentalmente de los procesos energéticos y metabólicos del organismo." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Capacidades_físicas_coordinativas
:Capacidades_físicas_coordinativas rdf:type owl:Class ;
                                   rdfs:subClassOf :Capacidades_físicas ;
                                   rdfs:comment "Son aquellas que regulan y organizan el movimiento, permitiendo una ejecución eficaz, económica y adaptada a diferentes situaciones. Dependen más del sistema nervioso central." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Contexto_discapacidades
:Contexto_discapacidades rdf:type owl:Class ;
                         rdfs:subClassOf :Problematicas ;
                         rdfs:comment "Trata las problematicas referentes a la existencia de discapaciades." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Contexto_edad
:Contexto_edad rdf:type owl:Class ;
               rdfs:subClassOf :Problematicas .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Contexto_enfermedades
:Contexto_enfermedades rdf:type owl:Class ;
                       rdfs:subClassOf :Problematicas ;
                       rdfs:comment "Problemáticas producidas debido a enfermedades." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Contexto_género
:Contexto_género rdf:type owl:Class ;
                 rdfs:subClassOf :Problematicas ;
                 rdfs:comment "Engloba todas las cuestiones referentes al género biológico (masculino y femenino)." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Contexto_lesiones
:Contexto_lesiones rdf:type owl:Class ;
                   rdfs:subClassOf :Problematicas ;
                   rdfs:comment "Problemáticas producidas por lesiones en los alumnos." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Contexto_sociales_y_culturales
:Contexto_sociales_y_culturales rdf:type owl:Class ;
                                rdfs:subClassOf :Problematicas ;
                                rdfs:comment "Problematicas surgidas de aspectos sociales y culturales (idioma, uso de prendas religiosas, separación entre géneros, etc.)" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Defensas
:Defensas rdf:type owl:Class ;
          rdfs:subClassOf :Fundamentos_tecnicos ;
          rdfs:comment "Tipos de defensa que realiza tori." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Deficiencia_grave
:Deficiencia_grave rdf:type owl:Class ;
                   rdfs:subClassOf :Impacto_funcional ;
                   rdfs:comment "Afecta de forma negativa y gravemente a la función corporal." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Deficiencia_ligera
:Deficiencia_ligera rdf:type owl:Class ;
                    rdfs:subClassOf :Impacto_funcional ;
                    rdfs:comment "Afecta de forma negativa a la función corporal, ligeramente." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Deficiencia_moderada
:Deficiencia_moderada rdf:type owl:Class ;
                      rdfs:subClassOf :Impacto_funcional ;
                      rdfs:comment "Afecta de forma negativa a la función corporal, de forma moderada." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Desplazamiento_de_rodillas
:Desplazamiento_de_rodillas rdf:type owl:Class ;
                            rdfs:subClassOf :Desplazamientos .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Desplazamientos
:Desplazamientos rdf:type owl:Class ;
                 rdfs:subClassOf :Fundamentos_tecnicos ;
                 rdfs:comment "Tipos de desplazamientos." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Edad_11a12
:Edad_11a12 rdf:type owl:Class ;
            rdfs:subClassOf :Etapas_edad ;
            rdfs:comment "Tercera etapa escolar de Primaria: 11 y 12 años." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Edad_6a8
:Edad_6a8 rdf:type owl:Class ;
          rdfs:subClassOf :Etapas_edad ;
          rdfs:comment "Primera etapa escolar de Primaria: 6 a 8 años." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Edad_9a10
:Edad_9a10 rdf:type owl:Class ;
           rdfs:subClassOf :Etapas_edad ;
           rdfs:comment "Segunda etapa escolar de Primaria: 9 y 10 años." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Enseñanza
:Enseñanza rdf:type owl:Class ;
           rdfs:subClassOf :Aikido ;
           rdfs:comment "Parte operativa o de gestión, más orientada a la organización escolar (alumnos, horarios, sesiones) que a los conceptos del arte marcial en sí." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Estructuras_clase
:Estructuras_clase rdf:type owl:Class ;
                   rdfs:subClassOf :Enseñanza ;
                   rdfs:comment "Estructura de la clase (apartados y orden de los mismos): saludo inicial, calentamiento, apartado técnico, estiramientos, saludo final." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Etapas_clase
:Etapas_clase rdf:type owl:Class ;
              rdfs:subClassOf :Enseñanza ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :forma_parte_de ;
                                owl:someValuesFrom :Estructuras_clase
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :tiene_apartado ;
                                owl:someValuesFrom :Apartados_clase
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :tiene_orden ;
                                owl:someValuesFrom xsd:integer
                              ] ;
              rdfs:comment "Son cada una de las etapas o pasos que componen la estructura de una clase" .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Etapas_edad
:Etapas_edad rdf:type owl:Class ;
             rdfs:subClassOf :Contexto_edad ;
             rdfs:comment "Etapas del desarrollo de las personas debido a la edad." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Facilitador_fuerte
:Facilitador_fuerte rdf:type owl:Class ;
                    rdfs:subClassOf :Impacto_funcional ;
                    rdfs:comment "Facilita la función corporal de forma fuerte." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Facilitador_leve
:Facilitador_leve rdf:type owl:Class ;
                  rdfs:subClassOf :Impacto_funcional ;
                  rdfs:comment "Facilita la función corporal de forma leve." .


###  http://www.semanticweb.org/juancarlos/ontologies/2025/4/aikido#Facilitador_moderado
:Facilitador_moderado rdf:type owl:Class ;
                      rdfs:subClassOf :Impacto_funcional ;
